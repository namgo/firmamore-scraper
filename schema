ALTER TABLE ONLY product
    ADD CONSTRAINT product_pkey PRIMARY KEY (id);


--
-- Name: uniq_hash; Type: CONSTRAINT; Schema: public; Owner: firmamore; Tablespace:
--

ALTER TABLE ONLY image
    ADD CONSTRAINT uniq_hash UNIQUE (hash);


--
-- Name: idx_object_hash; Type: INDEX; Schema: public; Owner: firmamore; Tablespace:
--

CREATE INDEX idx_object_hash ON object USING btree (hash);


--
-- Name: object_to_image_iid_idx; Type: INDEX; Schema: public; Owner: firmamore; Tablespace:
--

CREATE INDEX object_to_image_iid_idx ON object_to_image USING btree (iid);


--
-- Name: object_to_image_iid_idx1; Type: INDEX; Schema: public; Owner: firmamore; Tablespace:
